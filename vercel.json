{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".vercel/output",
  "functions": {
    "src/pages/api/**/*.{js,ts}": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/docs/api-spec.yaml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-yaml; charset=utf-8"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
