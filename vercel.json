{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/_astro/.*", "dest": "/_astro/$1" },
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
