{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@astrojs/vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_astro/.+\\.(js|css|svg|png|jpg|jpeg|gif|ico|webp|avif)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
